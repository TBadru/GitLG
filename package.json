{
	"name": "git-log--graph",
	"description": "git log --graph, an interactive Git Graph frontend",
	"version": "0.0.1",
	"icon": "logo.png",
	"publisher": "phil294",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/phil294/git-log--graph"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:git-log--graph.start"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "git-log--graph.start",
				"title": "Open graph view",
				"category": "git log --graph"
			}
		],
		"configuration": {
			"title": "git log --graph",
			"properties": {
				"git-log--graph.folder": {
					"description": "The desired *absolute* path in which a .git folder is located. Multi-root workspaces (multiple or nested folders) are currently not supported. If this option is not specified, the root of the first available workspace folder is used.",
					"type": "string"
				},
				"git-log--graph.actions.global": {
					"description": "Please check the extension's README for documentation. BE CAREFUL EDITING THESE OPTIONS.",
					"type": "array",
					"items": {
						"type": "object",
						"required": [
							"title",
							"args"
						],
						"properties": {
							"title": {
								"type": "string"
							},
							"description": {
								"type": "string"
							},
							"immediate": {
								"type": "boolean"
							},
							"ignore_errors": {
								"type": "boolean"
							},
							"args": {
								"type": "string"
							},
							"params": {
								"type": "array",
								"items": {
									"type": "string"
								}
							},
							"options": {
								"type": "array",
								"items": {
									"type": "object",
									"required": [
										"value",
										"default_active"
									],
									"properties": {
										"value": {
											"type": "string"
										},
										"default_active": {
											"type": "boolean"
										}
									}
								}
							}
						}
					},
					"default": [
						{
							"title": "‚Üß",
							"description": "Stash (include untracked)",
							"args": "stash -u",
							"options": [
								{
									"value": "--include-untracked",
									"default_active": true
								}
							],
							"immediate": true
						},
						{
							"title": "‚Ü•",
							"description": "Stash pop",
							"args": "stash pop",
							"immediate": true
						},
						{
							"title": "‚á©",
							"description": "Fetch all",
							"args": "fetch --all",
							"immediate": true
						},
						{
							"title": "üõá",
							"description": "Abort merge or cherry-pick",
							"args": "merge --abort; git cherry-pick --abort",
							"immediate": true,
							"ignore_errors": true
						}
					]
				},
				"git-log--graph.actions.commit": {
					"description": "Please check the extension's README for documentation. BE CAREFUL EDITING THESE OPTIONS.",
					"type": "array",
					"items": {
						"type": "object",
						"required": [
							"title",
							"args"
						],
						"properties": {
							"title": {
								"type": "string"
							},
							"description": {
								"type": "string"
							},
							"immediate": {
								"type": "boolean"
							},
							"ignore_errors": {
								"type": "boolean"
							},
							"args": {
								"type": "string"
							},
							"params": {
								"type": "array",
								"items": {
									"type": "string"
								}
							},
							"options": {
								"type": "array",
								"items": {
									"type": "object",
									"required": [
										"value",
										"default_active"
									],
									"properties": {
										"value": {
											"type": "string"
										},
										"default_active": {
											"type": "boolean"
										}
									}
								}
							}
						}
					},
					"default": [
						{
							"title": "‚Üí   Checkout",
							"immediate": true,
							"args": "checkout '$1'",
							"params": [
								"{COMMIT_HASH}"
							]
						},
						{
							"title": "+   Create branch",
							"args": "branch '$1' '$2'",
							"params": [
								"new_branch_name",
								"{COMMIT_HASH}"
							]
						},
						{
							"title": "ñ££   Cherry pick",
							"args": "cherry-pick '$1'",
							"params": [
								"{COMMIT_HASH}"
							],
							"options": [
								{
									"value": "--no-commit",
									"default_active": false
								}
							]
						},
						{
							"title": "‚éå   Revert",
							"args": "revert '$1'",
							"params": [
								"{COMMIT_HASH}"
							],
							"options": [
								{
									"value": "--no-commit",
									"default_active": false
								}
							]
						}
					]
				},
				"git-log--graph.actions.stash": {
					"description": "Please check the extension's README for documentation. BE CAREFUL EDITING THESE OPTIONS.",
					"type": "array",
					"items": {
						"type": "object",
						"required": [
							"title",
							"args"
						],
						"properties": {
							"title": {
								"type": "string"
							},
							"description": {
								"type": "string"
							},
							"immediate": {
								"type": "boolean"
							},
							"ignore_errors": {
								"type": "boolean"
							},
							"args": {
								"type": "string"
							},
							"params": {
								"type": "array",
								"items": {
									"type": "string"
								}
							},
							"options": {
								"type": "array",
								"items": {
									"type": "object",
									"required": [
										"value",
										"default_active"
									],
									"properties": {
										"value": {
											"type": "string"
										},
										"default_active": {
											"type": "boolean"
										}
									}
								}
							}
						}
					},
					"default": [
						{
							"title": "‚Üí   Apply",
							"immediate": true,
							"args": "stash apply '$1'",
							"params": [
								"{COMMIT_HASH}"
							]
						}
					]
				},
				"git-log--graph.actions.branch": {
					"description": "Please check the extension's README for documentation. BE CAREFUL EDITING THESE OPTIONS.",
					"type": "array",
					"items": {
						"type": "object",
						"required": [
							"title",
							"args"
						],
						"properties": {
							"title": {
								"type": "string"
							},
							"description": {
								"type": "string"
							},
							"immediate": {
								"type": "boolean"
							},
							"ignore_errors": {
								"type": "boolean"
							},
							"args": {
								"type": "string"
							},
							"params": {
								"type": "array",
								"items": {
									"type": "string"
								}
							},
							"options": {
								"type": "array",
								"items": {
									"type": "object",
									"required": [
										"value",
										"default_active"
									],
									"properties": {
										"value": {
											"type": "string"
										},
										"default_active": {
											"type": "boolean"
										}
									}
								}
							}
						}
					},
					"default": [
						{
							"title": "‚Üí   Checkout",
							"description": "",
							"immediate": true,
							"args": "checkout '$1'",
							"params": [
								"{BRANCH_NAME}"
							]
						},
						{
							"title": "‚õô   Merge",
							"args": "merge '$1'",
							"params": [
								"{BRANCH_NAME}"
							],
							"options": [
								{
									"value": "--no-commit",
									"default_active": false
								}
							]
						},
						{
							"title": "‚úé   Rename",
							"args": "branch -m '$1' '$2'",
							"params": [
								"{BRANCH_NAME}",
								"new_branch_name"
							]
						},
						{
							"title": "üóë   Delete",
							"args": "branch -d '$1'",
							"params": [
								"{BRANCH_NAME}"
							],
							"options": [
								{
									"value": "--force",
									"default_active": false
								}
							]
						},
						{
							"title": "‚•Å   Rebase",
							"args": "rebase '$1'",
							"params": [
								"{BRANCH_NAME}"
							]
						}
					]
				}
			}
		}
	},
	"scripts": {},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.47.0",
		"@types/vscode-webview": "^1.57.0",
		"coffeescript": "^2.7.0"
	}
}
